{
  "name": "koa-req-logger",
  "version": "1.5.2",
  "description": "A koa logging middleware, logs requests and errors. Also adds date and x-response-time to response headers.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "pretest": "yarn lint",
    "test": "jest",
    "build": "tsc",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@types/jest": "^24.0.16",
    "pino": "^5.13.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/koa": "^2.0.49",
    "@types/pino": "^5.20.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "http-errors": "^1.7.3",
    "jest": "^24.8.0",
    "koa": "^2.7.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "bail": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "repository": "github:DrBarnabus/koa-req-logger",
  "author": "DrBarnabus <drbarnabus@drbarnabus.com> (https://drbarnabus.com)",
  "license": "MIT",
  "keywords": [
    "koa",
    "logging",
    "logger",
    "request",
    "pino",
    "middleware",
    "X-Request-ID",
    "X-Response-Time",
    "http",
    "header",
    "json"
  ]
}
