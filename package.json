{
  "name": "koa-req-logger",
  "version": "1.2.2",
  "description": "A koa logging middleware, logs requests and errors. Also adds date and x-response-time to response headers.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "dependencies": {
    "pino": "^5.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/koa": "^2.0.46",
    "@types/pino": "^5.20.0",
    "@types/uuid": "^3.4.4",
    "eslint": "^5.2.0",
    "eslint-config-koa": "^2.0.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "http-errors": "^1.6.3",
    "jest": "^23.4.1",
    "koa": "^2.5.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "jest": {
    "testMatch": [
      "**/test/*.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "bail": true,
    "testEnvironment": "node",
    "coverage": true
  },
  "repository": "github:DrBarnabus/koa-req-logger",
  "author": "DrBarnabus <drbarnabus@drbarnabus.com> (https://drbarnabus.com)",
  "license": "MIT",
  "keywords": [
    "koa",
    "logging",
    "logger",
    "request",
    "pino",
    "middleware",
    "X-Request-ID",
    "X-Response-Time",
    "http",
    "header",
    "json"
  ]
}
